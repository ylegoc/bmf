<!DOCTYPE html>
<html>
  <head>
    <title>Plot</title>
    <!--<link rel='stylesheet' href='/stylesheets/style.css' />-->
    <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
  </head>
  <body>
    <div id="plot-div"></div>
    <script>
        console.log('test data');
        let datafile = JSON.parse('<%- datafile %>');
        let data = datafile.data;
        
        let trace = {
          x: data.x, 
          y: data.y, 
          type: 'scatter'
        };

        let plotData = [trace];
        Plotly.newPlot('plot-div', plotData);
    </script>
  </body>
</html>